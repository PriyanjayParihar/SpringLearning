<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
   <bean class="org.example.Marks" name="marks">
<!--       <property name="p" value="90"/>-->
<!--       <property name="c" value="75"/>-->
<!--       <property name="m" value="100"/>-->
        <constructor-arg>
            <value>90</value>

        </constructor-arg>
       <constructor-arg>
           <value type="int">70</value>
<!--        constructor arg-->
       </constructor-arg>
       <constructor-arg type="int" value="100"/>
   </bean>
<!--    setter injection-->
    <bean class="org.example.BeanWithCollectionAndRef.StudentAllCollectionAndRef" name="student1">
       <property name="name" value="ravi"/>
       <property name="phones" >
           <list>
               <value>251252</value>
               <value>2452552</value>
               <value>274552</value>
               <null/>
           </list>
       </property>
       <property name="cities">
           <set>
               <value>bhopal</value>
               <value>delhi</value>
           </set>
       </property>
       <property name="courses">
           <map>
               <entry key="java" value="1 month"/>
               <entry key="python" value="2 month"/>
           </map>
       </property>
       <property name="marks" ref="marks"/>
<!--        <property name="marks">-->
<!--            <ref bean="marks"/>-->
<!--        </property>-->
   </bean>
</beans>